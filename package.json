{
  "name": "@chessire/pieces",
  "version": "1.0.2",
  "description": "React chess pieces component",
  "keywords": [
    "chess",
    "piece",
    "react",
    "component"
  ],
  "files": [
    "dist/**/*.js",
    "dist/esm/*.d.ts",
    "README.md",
